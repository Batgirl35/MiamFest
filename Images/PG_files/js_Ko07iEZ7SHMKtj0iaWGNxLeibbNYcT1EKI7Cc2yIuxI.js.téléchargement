/**
 * @file
 * File containing code used to track booking via partners.
 */

(function ($) {

  // Code responsible to track partners.
  Drupal.behaviors.mrBookingTracking = {
    attach: function (context, settings) {
      $('body').once('coffee', function() {
        var indirectExists = /[?&]indirect=/.test(location.search);
        var referrerExists = typeof document.referrer !== 'undefined' && !Drupal.urlIsLocal(document.referrer);
        var booking_channels = settings.mr_b2c_booking.booking_channels;
        var default_connection_id = settings.mr_b2c_booking.default_connection_id;
        var expiry = new Date();
        expiry.setTime(expiry.getTime() + (settings.mr_b2c_booking.cookie_lifetime * 60 * 60 * 1000));
        if (indirectExists) {
          // If there is an indirect code we use this.
          var sParam = 'indirect';
          var sPageURL = window.location.search.substring(1);
          var sURLVariables = sPageURL.split('&');
          for (var i = 0; i < sURLVariables.length; i++) {
            var sParameterName = sURLVariables[i].split('=');
            if (sParameterName[0] == sParam) {
              var indirect = sParameterName[1];
            }
          }
          var argument = 'indirect=' + indirect;
          booking_channels.forEach(function (item, index) {
            if (item.arguments === argument) {
              $.cookie('connection_id', item.connection_id, { expires: expiry, path: '/' });
            }
          });
        } else {
          if (referrerExists) {
            var referrer = getLocation(document.referrer).hostname;
            booking_channels.forEach(function (item, index) {
              if (item.arguments.includes('indirect') == false) {
                if (getLocation(item.arguments).hostname == referrer) {
                  $.cookie('http_referrer', referrer, {path: '/'});
                  $.cookie('connection_id', item.connection_id, { expires: expiry, path: '/' });
                }
              }
            });
          }
        }
      });
    }
  };

  var getLocation = function(href) {
    if (href.includes('http') == false) {
      href = 'https://' + href;
    }
    var l = document.createElement("a");
    l.href = href;
    return l;
  }

})(jQuery);
